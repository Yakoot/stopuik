<urlrewrite use-query-string="false">
    <rule>
        <from>/login.*</from>
        <to>/auth.html</to>
        <set type="content-type">text/html; charset=utf-8</set>
    </rule>
    <rule>
        <from>^/edit$</from>
        <to>/edit.html</to>
        <set type="content-type">text/html; charset=utf-8</set>
    </rule>
</urlrewrite>
